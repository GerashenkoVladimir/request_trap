<!DOCTYPE html>
<html>
  <head>
    <title>RequestTrap</title>
    <%= csrf_meta_tags %>
    <%= action_cable_meta_tag %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <style>
      table, th, td {
        border: 1px solid black;
      }
      table p {
        margin: 0;
      }
    </style>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
